pipeline {
 agent any
  stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/sachinshaji/airbus-project.git'
                sh 'ls -latrh'
            }
        }
        stage('Docker build') {
            steps {
                sh "cd /var/lib/jenkins/workspace/airbus-jenkins-pipeline"
                sh 'ls -latr'
            }
        }
    }
}
