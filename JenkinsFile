pipeline {
 agent any
  stages {
        stage('Checkout') {
            steps {
                sh 'git clone https://github.com/sachinshaji/airbus-project.git'
                sh 'cd airbus-project'
            }
        }
        stage('Docker build') {
            steps {
                sh 'docker build --tag python-docker .'
                sh 'docker run -d -p 5000:5000 python-docker'
            }
        }
    }
}
