pipeline {
 agent any
  stages {
        stage('Checkout') {
            steps {
               // sh 'git clone https://github.com/sachinshaji/airbus-project.git'
                sh 'ls -latr'
            }
        }
        stage('Docker build') {
            steps {
                sh 'sudo docker build --tag python-docker .'
                sh 'sudo docker run -d -p 5000:5000 python-docker'
            }
        }
    }
}
